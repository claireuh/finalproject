import requests
import matplotlib.pyplot as plt
import os
import sqlite3
import unittest
import numpy as np
import json


def setUpDatabase(db_name):
    path = os.path.dirname(os.path.abspath(__file__))
    conn = sqlite3.connect(path+'/'+db_name)
    cur = conn.cursor()
    return cur, conn

#f346c59968a4bc43d890eaacf2a45895

def get_sstock():
    url = 'http://api.marketstack.com/v1/eod?access_key=f346c59968a4bc43d890eaacf2a45895&symbols=AAPL'
    response = requests.get(url)
    data = response.text
    data_list = json.loads(data)
    return data_list





def main():
    print(get_sstock())

    
    
if __name__ == "__main__":
    main()